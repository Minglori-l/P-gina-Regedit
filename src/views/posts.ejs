<%- include('components/header.ejs'); %>
<%- include('components/nav.ejs'); %>
<% if (menssage) { %>
    <%- include('components/alertSucces.ejs', {menssage: menssage}); %>
<% } %>
<main class="w-full h-auto px-8 py-12">
    <h1 class="text-3xl font-medium flex flex-col gap-8">
        <%= title %>
        <ul class="grid grid-cols-2 gap-5">
            <% posts.forEach(function(post){ %>
                <%- include('components/cardPosts.ejs', {post: post}); %>
            <% }); %>
        </ul>
    </h1>
</main>
<%- include('components/modalAddPosts.ejs'); %>
<%- include('components/modalAddUsers.ejs'); %>
<%- include('components/footer.ejs'); %>